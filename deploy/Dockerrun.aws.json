{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "authentication": {
    "bucket": "creds.weeyuck.com",
    "key": "dockercfg"
  },
  "volumes": [
    {
      "name": "site-key-holder",
      "host": {
        "sourcePath": "/var/app/current/data"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "weeyuck",
      "image": "quay.io/horrorcheck/weeyuck:ac2cd84ee9705b238b4c599313ff34492a6e45d0",
      "essential": true,
      "memory": 256,
      "mountPoints": [
        {
          "sourceVolume": "site-key-holder",
          "containerPath": "/srv/data"
        },
        {
          "sourceVolume": "awseb-logs-weeyuck",
          "containerPath": "/srv/log"
        }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8000
        }
      ]
    }
  ]
}
